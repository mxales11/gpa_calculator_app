<div class="container-fluid">
    <div class="row-fluid">
       
        <div class="span8">
            <!--Body content-->
             <div class="row-fluid">
                    <ul class="nav nav-tabs">
                        <li>
                            <a data-toggle="tab" class="btn btn-large btn-primary" href="#tab1"><abbr title="Ballet">Target cumulative GPA</abbr></a>
                        </li>
                        <li>
                            <a data-toggle="tab" class="btn btn-large btn-primary" href="#tab2"><abbr title="Modern Dance">Target major GPA</abbr></a>
                        </li>
                        <li>
                            <a data-toggle="tab" class="btn btn-large btn-primary" href="#tab3"><abbr title="Zumba Dance">Predicted GPA</abbr></a>
                        </li>
                    </ul>
                    <div class="tab-content">
                        <div class="tab-pane" id="tab1">
                            <p><%="FORM FOR TARGET CUMULATIVE GPA" %></p>
                              <%= link_to 'Make projections', new_student_projection_path(@student, @projection) %>
                        </div>
                        <div class="tab-pane" id="tab2">
                            <p><%="FORM FOR TARGET MAJOR GPA" %></p>
                              <%= link_to 'Make projections', new_student_projection_path(@student, @projection) %>
                        </div>
                        <div class="tab-pane" id="tab3">
                            <p><%="FORM FOR PREDICTED GPA" %></p>
                              <%= link_to 'Make projections', new_student_projection_path(@student, @projection) %>    
                        </div>
                    </div>
                </div>
                    <div>
                        <%= link_to 'Edit', edit_student_path(@student) %> |
                        <%= link_to 'Back', students_path %>
                    </div>
        </div>

         <div class="span4" id = "sidebar">
            <!--Sidebar content-->
               <br>
                    <%= render 'display_courses' %> 
                </br>
                <br>
                    <h5>
                        <%= link_to 'Add completed course', new_student_taken_course_path(@student, @taken_course) %>
                    <h5>
               </br>
        </div>
    </div>
</div>




